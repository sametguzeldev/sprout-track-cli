{
  "name": "sprout-track-cli",
  "version": "1.0.0",
  "description": "Command-line interface for Sprout-Track baby tracking application",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sprout-track": "./bin/sprout-track.js"
  },
  "scripts": {
